<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ajayrwarrier.mycityapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainAttractions" android:label="@string/MainAttactions"></activity>
        <activity android:name=".Malls" android:label="@string/events"></activity>
        <activity android:name=".Hospitals" android:label="@string/Hospital"></activity>
        <activity android:name=".Restaurants" android:label="@string/Restaurants"></activity>

    </application>

</manifest>